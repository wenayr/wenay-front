    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm": "^11.6.1",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "^19.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
