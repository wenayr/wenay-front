{
  "name": "client",
  "version": "1.0.0",
  "scripts": {
    "start-bin2": "NODE_ENV=production npm start",
    "lint2": "eslint src --fix",
    "buildWebpack": "webpack --mode production",
    "start": "react-app-rewired start",
    "buildSite": "react-app-rewired build",
    "start_NoEslint": "set DISABLE_ESLINT_PLUGIN=true&& react-app-rewired start",
    "build-bin2": "NODE_ENV=production node --loader ts-node/esm ./build.ts && rollup -c",
    "build": "react-app-rewired build",
    "addBuildToGit": "git add build && git commit -a -m \"add build folder clientBacktest\" && git push origin history",
    "buildWin": "set \"GENERATE_SOURCEMAP=false\" && react-app-rewired build && npm run addBuildToGit",
    "buildMac": "export \"GENERATE_SOURCEMAP=false\" && react-app-rewired build && npm run addBuildToGit",
    "nomI": "npm i",
    "update": "npm update"
  },
  "dependencies": {
    "path-browserify": "^1.0.1",
    "re-resizable": "^6.9.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-rnd": "^10.4.11",
    "socket.io-client": "^4.7.5",
    "ag-grid-community": "^31.3.2",
    "ag-grid-react": "^31.3.2",
    "lightweight-charts": "^4.1.6",
    "wenay-common": "^1.0.149"
  },
  "devDependencies": {
    "@types/node": "^18.19.39",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "dotenv": "^8.2.0",
    "react-app-rewired": "^2.2.1",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      ">1%"
    ]
  }
}
